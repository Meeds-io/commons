<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Añadir una página</AddPage>
         <EditNavigation>Editar Navegación</EditNavigation>
         <Content>Editar en línea</Content>
         <Editor>Editar</Editor>
         <Page>Página</Page>
         <Site>Sitio</Site>
         <Layout>Editar diseño</Layout>
         <siteLayout>Editar Diseño</siteLayout>
         <ManagePages>Administrar páginas</ManagePages>
         <GroupSite>Sitio de grupo</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Añadir sitio</AddSite>
      </action>
<ManageSites>Administrar sitios</ManageSites>
<ManageGroupSites>Administrar sitios del grupo</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Título:</title>  
      <description>Descripción:</description>
      <keywords>Palabras Clave:</keywords>
      <language>idioma:</language>
      <robots>Robots:</robots>
      <sitemap>Mapa del sitio:</sitemap>
      <sitemapvisible>Visible en el mapa del sitio</sitemapvisible>
  <isInherited>Heredado de la página principal</isInherited>
      <priority>Prioridad:</priority>
      <frequency>Frecuencia:</frequency>
      <select-language>Selecciona Idioma</select-language>
      <option>
        <Always>Siempre</Always>
        <Hourly>Por hora</Hourly>
        <Daily>Diario</Daily>
        <Weekly>Semanalmente</Weekly>
        <Monthly>Mensualmente</Monthly>
        <Yearly>Anualmente</Yearly>
        <Never>Nunca</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Se produjo un error inesperado. No puede guardar los metadatos de la página.</repository-exception>
      <confirm-delete>¿Está seguro que desea eliminar estos datos SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Borrar</seo-delete>
    </title>
<help> 
  <priority>Valores válidos oscilan entre 0.0 y 1.0.</priority>
  <description>Ayudar a los buscadores: hacerles saber que la siguiente página tiene un enlace que están buscando.</description>
  <keywords>Términos más importantes en la página deben estar separados por una coma.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Gestión de SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>El campo de prioridad tiene un valor no válido. Por favor, introduzca un número entre 0.0 y 1.1.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>italiano</it>      
      <fr>Francés</fr>
      <en>Inglés</en>      
    </label>
  </Locale>
  
</bundle>
