<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Dodaj stran</AddPage>
         <EditNavigation>Uredi krmarjenje</EditNavigation>
         <Content>Urejanje v vrstici</Content>
         <Editor>Uredi</Editor>
         <Page>Stran</Page>
         <Site>Spletno mesto</Site>
         <Layout>Uredi postavitev</Layout>
         <siteLayout>Uredi postavitev</siteLayout>
         <ManagePages>Upravljanje strani</ManagePages>
         <GroupSite>Mesto skupine</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Dodaj spletno mesto</AddSite>
      </action>
<ManageSites>Upravljanje spletnih mest</ManageSites>
<ManageGroupSites>Upravljanje spletnega mesta skupine</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Naslov:</title>  
      <description>Opis:</description>
      <keywords>Ključne besede:</keywords>
      <language>Jezik:</language>
      <robots>Roboti:</robots>
      <sitemap>Zemljevid strani:</sitemap>
      <sitemapvisible>Vidno v zemljevidu strani</sitemapvisible>
  <isInherited>Podedovano od nadrejene strani</isInherited>
      <priority>Prioriteta:</priority>
      <frequency>Pogostost:</frequency>
      <select-language>Izberite jezik</select-language>
      <option>
        <Always>Vedno</Always>
        <Hourly>Vsako uro</Hourly>
        <Daily>Dnevno</Daily>
        <Weekly>Tedensko</Weekly>
        <Monthly>Mesečno</Monthly>
        <Yearly>Letno</Yearly>
        <Never>Nikoli</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Pripetila se je nepričakovana napaka. Metapodatkov strani ni mogoče shraniti.</repository-exception>
      <confirm-delete>Ali ste prepričani, da želite izbrisati te SEO podatke?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Izbriši</seo-delete>
    </title>
<help> 
  <priority>Veljavne vrednosti so med 0.0 in 1.0.</priority>
  <description>Pomoč iskalcem: naj vedo, da naslednja stran vsebuje povezavo, ki jo iščejo.</description>
  <keywords>Najpomembnejši izrazi na strani morajo biti ločene z vejico.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Upravljanje SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Prednostno polje ima neveljavno vrednost. Vnesite številko med 0,0 in 1,0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italijanščina</it>      
      <fr>Francoščina</fr>
      <en>Angleščina</en>      
    </label>
  </Locale>
  
</bundle>
