<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Ajouter</AddPage>
         <EditNavigation>Navigation</EditNavigation>
         <Content>Modification en ligne</Content>
         <Editor>Modifier</Editor>
         <Page>Page</Page>
         <Site>Site</Site>
         <Layout>Mise en Page</Layout>
         <siteLayout>Mise en Page</siteLayout>
         <ManagePages>Pages</ManagePages>
         <GroupSite>Site de Groupe</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Ajouter un Site</AddSite>
      </action>
<ManageSites>Gestion des Sites</ManageSites>
<ManageGroupSites>Gestion des Sites de Groupe</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Titre:</title>  
      <description>Description:</description>
      <keywords>Mots clés:</keywords>
      <language>Langue:</language>
      <robots>Robots:</robots>
      <sitemap>Sitemap:</sitemap>
      <sitemapvisible>Visible dans le sitemap</sitemapvisible>
  <isInherited>Hérité de la page parent</isInherited>
      <priority>Priorité:</priority>
      <frequency>Fréquence:</frequency>
      <select-language>Sélectionnez une langue</select-language>
      <option>
        <Always>Toujours</Always>
        <Hourly>Toutes les heures</Hourly>
        <Daily>Quotidien</Daily>
        <Weekly>Hebdomadaire</Weekly>
        <Monthly>Mensuel</Monthly>
        <Yearly>Annuel</Yearly>
        <Never>Jamais</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Une erreur inattendue est apparue lors de l'enregistrement des métadonnées.</repository-exception>
      <confirm-delete>Êtes-vous sûr de vouloir supprimer les données SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Supprimer</seo-delete>
    </title>
<help> 
  <priority>Valeurs valides de 0.0 à 1.0.</priority>
  <description>Aidez les utilisateurs a savoir en quoi cette page a un lien avec ce qu'ils recherchent.</description>
  <keywords>Les termes les plus importants de la page doivent être séparés par une virgule.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Gestion SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Le champ Priorité a une valeur invalide. Veuillez saisir un nombre compris entre 0.0 et 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italien</it>      
      <fr>Français</fr>
      <en>Anglais</en>      
    </label>
  </Locale>
  
</bundle>
