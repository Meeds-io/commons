<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Aggiungere Pagina</AddPage>
         <EditNavigation>Navigazione</EditNavigation>
         <Content>Contenuto</Content>
         <Editor>Modificare</Editor>
         <Page>Pagina</Page>
         <Site>Sito</Site>
         <Layout>Modificare il Layout</Layout>
         <siteLayout>Impaginazione</siteLayout>
         <ManagePages>Gestire Pagine</ManagePages>
         <GroupSite>Sito di Gruppo</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Aggiungere sito</AddSite>
      </action>
<ManageSites>Gestire siti</ManageSites>
<ManageGroupSites>Gestire siti di gruppo</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Titolo:</title>  
      <description>Descrizione:</description>
      <keywords>Parole Chiave:</keywords>
      <language>Lingua:</language>
      <robots>Robot:</robots>
      <sitemap>Mappa del sito:</sitemap>
      <sitemapvisible>Visibile nella mappa del sito</sitemapvisible>
  <isInherited>Ereditato dalla pagina padre</isInherited>
      <priority>Priorità:</priority>
      <frequency>Ferequenza:</frequency>
      <select-language>Selezionare una lingua</select-language>
      <option>
        <Always>Sempre</Always>
        <Hourly>Ogni ora</Hourly>
        <Daily>Quotidianamente</Daily>
        <Weekly>Settimanalmente</Weekly>
        <Monthly>Mensilmente</Monthly>
        <Yearly>Annualmente</Yearly>
        <Never>Mai</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>Un errore inaspettato! Impossibile salvare i meta dati della pagina.</repository-exception>
      <confirm-delete>Sei sicuro di voler cancellare questi dati SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Cancellare</seo-delete>
    </title>
<help> 
  <priority>Intervallo di valori validi da 0.0 a 1.0.</priority>
  <description>Aiuta i ricercatori: fagli sapere che la pagina contiene un collegamento con quello che stanno cercando.</description>
  <keywords>Termini più importanti nella pagina devono essere separati da una virgola.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Gestione SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Il campo Priorità contiene un valore float non valido. Intervallo di valori validi è da 0.0 a 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italiano</it>      
      <fr>Francese</fr>
      <en>Inglese</en>      
    </label>
  </Locale>
  
</bundle>
