			<adapter>
				(function($) {
				var oldJQuery = this.jQuery;
				this.jQuery = $;

				<include>/javascript/eXo/suggester/selectize.js</include>

				var selectize = this.Selectize;
				this.jQuery = oldJQuery;
				return selectize;
				})($);
			</adapter>
		</script>
